{
  "singleQuote": true,
  "printWidth": 9999,
  "trailingComma": "none",
  "useTabs": false,
  "tabWidth": 2,
  "semi": false,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "bracketSpacing": false,
  "bracketSameLine": true,
  "arrowParens": "avoid",
  "proseWrap": "never",
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "lf",
  "singleAttributePerLine": false
}
